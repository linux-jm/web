#: original/man1/find.1:1577
#, no-wrap
msgid "j"
msgstr ""

#. type: Plain text
#: original/man1/find.1:1579
msgid "day of year (001..366)"
msgstr ""

#. type: Plain text
#: original/man1/find.1:1581
msgid "month (01..12)"
msgstr ""

