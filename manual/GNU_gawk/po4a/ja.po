msgstr "I<r>B<{>I<n>B<,}>"

#. type: TP
#: original/man1/gawk.1:1592
#, no-wrap
msgid "I<r>B<{>I<n>B<,>I<m>B<}>"
msgstr "I<r>B<{>I<n>B<,>I<m>B<}>"

#. type: Plain text
#: original/man1/gawk.1:1613
#, fuzzy
#| msgid ""
