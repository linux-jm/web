#. type: Plain text
#: build/C/man2/io_submit.2:160
#, no-wrap
msgid ""
"enum {\n"
"    IOCB_CMD_PREAD = 0,\n"
"    IOCB_CMD_PWRITE = 1,\n"
"    IOCB_CMD_FSYNC = 2,\n"
"    IOCB_CMD_FDSYNC = 3,\n"
"    IOCB_CMD_POLL = 5,\n"
"    IOCB_CMD_NOOP = 6,\n"
"    IOCB_CMD_PREADV = 7,\n"
"    IOCB_CMD_PWRITEV = 8,\n"
"};\n"
msgstr ""
"enum {\n"
"    IOCB_CMD_PREAD = 0,\n"
"    IOCB_CMD_PWRITE = 1,\n"
"    IOCB_CMD_FSYNC = 2,\n"
"    IOCB_CMD_FDSYNC = 3,\n"
"    IOCB_CMD_POLL = 5,\n"
"    IOCB_CMD_NOOP = 6,\n"
"    IOCB_CMD_PREADV = 7,\n"
"    IOCB_CMD_PWRITEV = 8,\n"
"};\n"

#. type: Plain text
