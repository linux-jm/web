#. type: Plain text
#: build/C/man3/strftime.3:726
#, no-wrap
msgid ""
"#include E<lt>time.hE<gt>\n"
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
msgstr ""

#. type: Plain text
#: build/C/man3/strftime.3:733
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    char outstr[200];\n"
"    time_t t;\n"
"    struct tm *tmp;\n"
msgstr ""

#. type: Plain text
#: build/C/man3/strftime.3:740
#, no-wrap
msgid ""
"    t = time(NULL);\n"
"    tmp = localtime(&t);\n"
"    if (tmp == NULL) {\n"
"        perror(\"localtime\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""

#. type: Plain text
#: build/C/man3/strftime.3:745
#, no-wrap
msgid ""
