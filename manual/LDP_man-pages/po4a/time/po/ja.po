#: build/C/man8/zic.8:472
#, no-wrap
msgid "Zone\tNAME\tSTDOFF\tRULES\tFORMAT\t[UNTIL]\n"
msgstr " Zone\tNAME\tSTDOFF\tRULES\tFORMAT\t[UNTIL]\n"

