#, no-wrap
msgid "\\w'Zone\\0\\0'u +\\w'Asia/Amman\\0\\0'u +\\w'STDOFF\\0\\0'u +\\w'Jordan\\0\\0'u +\\w'FORMAT\\0\\0'u"
msgstr " \\w'Zone\\0\\0'u +\\w'Asia/Amman\\0\\0'u +\\w'STDOFF\\0\\0'u +\\w'Jordan\\0\\0'u +\\w'FORMAT\\0\\0'u"

