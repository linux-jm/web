#. type: Plain text
#: build/C/man3/newlocale.3:292
msgid ""
"Specify the B<LC_TIME> setting as an empty string, which causes the value to "
"be taken from environment variable settings (which, here, specify I<mi_NZ>, "
"New Zealand Māori):"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:298
#, no-wrap
msgid ""
"$ LC_ALL=mi_NZ ./a.out fr_FR \"\"\n"
"123456,789\n"
"Te Paraire, te 07 o Poutū-te-rangi, 2014 00:38:44 CET\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:307
#, no-wrap
msgid ""
"#define _XOPEN_SOURCE 700\n"
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
"#include E<lt>locale.hE<gt>\n"
"#include E<lt>time.hE<gt>\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:319
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    char buf[100];\n"
"    time_t t;\n"
"    size_t s;\n"
"    struct tm *tm;\n"
"    locale_t loc, nloc;\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:324
#, no-wrap
msgid ""
"    if (argc E<lt> 2) {\n"
"        fprintf(stderr, \"Usage: %s locale1 [locale2]\\en\", argv[0]);\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:327
#, no-wrap
msgid ""
"    /* Create a new locale object, taking the LC_NUMERIC settings\n"
"       from the locale specified in argv[1] */\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:331
#, no-wrap
msgid ""
"    loc = newlocale(LC_NUMERIC_MASK, argv[1], (locale_t) 0);\n"
"    if (loc == (locale_t) 0)\n"
"        errExit(\"newlocale\");\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:337
#, no-wrap
msgid ""
"    /* If a second command-line argument was specified, modify the\n"
"       locale object to take the LC_TIME settings from the locale\n"
"       specified in argv[2]. We assign the result of this newlocale()\n"
"       call to \\(aqnloc\\(aq rather than \\(aqloc\\(aq, since in some "
"cases, we might\n"
"       want to preserve \\(aqloc\\(aq if this call fails. */\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:344
#, no-wrap
msgid ""
"    if (argc E<gt> 2) {\n"
"        nloc = newlocale(LC_TIME_MASK, argv[2], loc);\n"
"        if (nloc == (locale_t) 0)\n"
"            errExit(\"newlocale\");\n"
"        loc = nloc;\n"
"    }\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:346
#, no-wrap
msgid "    /* Apply the newly created locale to this thread */\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:348
#, no-wrap
msgid "    uselocale(loc);\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:350
#, no-wrap
msgid "    /* Test effect of LC_NUMERIC */\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:352
#, no-wrap
msgid "    printf(\"%8.3f\\en\", 123456.789);\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:354
#, no-wrap
msgid "    /* Test effect of LC_TIME */\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:359
#, no-wrap
msgid ""
"    t = time(NULL);\n"
"    tm = localtime(&t);\n"
"    if (tm == NULL)\n"
"        errExit(\"time\");\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:363
#, no-wrap
msgid ""
"    s = strftime(buf, sizeof(buf), \"%c\", tm);\n"
"    if (s == 0)\n"
"        errExit(\"strftime\");\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:365
#, no-wrap
msgid "    printf(\"%s\\en\", buf);\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:367
#, no-wrap
msgid "    /* Free the locale object */\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:370
#, no-wrap
msgid ""
"    uselocale(LC_GLOBAL_HANDLE);    /* So \\(aqloc\\(aq is no longer in use "
"*/\n"
"    freelocale(loc);\n"
msgstr ""

#. type: Plain text
#: build/C/man3/newlocale.3:381
msgid ""
"B<locale>(1), B<duplocale>(3), B<setlocale>(3), B<uselocale>(3), "
"B<locale>(5), B<locale>(7)"
msgstr ""

