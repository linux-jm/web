msgstr ""

#. type: Plain text
#: build/C/man5/proc.5:5518
msgid "See B<bpf>(2)."
msgstr ""

#. type: TP
#: build/C/man5/proc.5:5518
#, no-wrap
msgid "I</proc/sys/net/core/somaxconn>"
msgstr ""

#. type: Plain text
#: build/C/man5/proc.5:5527
