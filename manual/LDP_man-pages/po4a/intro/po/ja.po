#. type: TP
#: build/C/man7/man.7:157
#, no-wrap
msgid "B<\\&.RI>"
msgstr "B<\\&.RI>"

#. type: Plain text
#: build/C/man7/man.7:160
msgid "Roman alternating with italics"
msgstr "ローマンとイタリックとを交互に。"

#. type: TP
#: build/C/man7/man.7:160
#, no-wrap
msgid "B<\\&.SB>"
msgstr "B<\\&.SB>"

#. type: Plain text
#: build/C/man7/man.7:163
