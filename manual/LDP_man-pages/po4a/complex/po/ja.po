"B<catanhf>(),\n"
"B<catanhl>()"
msgstr ""
"B<catanh>(),\n"
"B<catanhf>(),\n"
"B<catanhl>()"

#. type: Plain text
#: build/C/man3/catanh.3:75
#, no-wrap
msgid ""
"    c = catanh(z);\n"
"    printf(\"catanh() = %6.3f %6.3f*i\\en\", creal(c), cimag(c));\n"
msgstr ""
"    c = catanh(z);\n"
"    printf(\"catanh() = %6.3f %6.3f*i\\en\", creal(c), cimag(c));\n"

#. type: Plain text
#: build/C/man3/catanh.3:78
#, no-wrap
msgid ""
"    f = 0.5 * (clog(1 + z) - clog(1 - z));\n"
"    printf(\"formula  = %6.3f %6.3f*i\\en\", creal(f2), cimag(f2));\n"
msgstr ""
"    f = 0.5 * (clog(1 + z) - clog(1 - z));\n"
"    printf(\"formula  = %6.3f %6.3f*i\\en\", creal(f2), cimag(f2));\n"

