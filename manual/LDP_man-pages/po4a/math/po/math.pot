msgid ""
"    printf(\"frexp(%g, &e) = %g: %g * %d\\(ha%d = %g\\en\",\n"
"           x, r, r, FLT_RADIX, exp, x);\n"
"    exit(EXIT_SUCCESS);\n"
"}\n"
