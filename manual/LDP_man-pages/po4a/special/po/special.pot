msgstr ""

#. type: Plain text
#: build/C/man4/vcs.4:105
msgid "You may do a screendump on vt3 by switching to vt1 and typing"
msgstr ""

#. type: Plain text
#: build/C/man4/vcs.4:109
#, no-wrap
msgid "cat /dev/vcs3 E<gt>foo\n"
msgstr ""

#. type: Plain text
#: build/C/man4/vcs.4:115
