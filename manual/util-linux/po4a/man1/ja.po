#: original/man1/lsfd.1:476
#, no-wrap
msgid "# lsfd -Q \\(aq(PID == 1) or (PID == 2)\\(aq\n"
msgstr "# lsfd -Q \\(aq(PID == 1) or (PID == 2)\\(aq\n"

#. type: Plain text
#: original/man1/lsfd.1:481 original/man1/lsfd.1:541 original/man1/lsfd.1:565
msgid "Do the same in an alternative way:"
msgstr "同じことを別の記法により行う。"

