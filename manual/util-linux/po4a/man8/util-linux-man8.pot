#. type: Plain text
#: original/man8/blockdev.8:123
msgid "B<--getro>"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:125
msgid "Get read-only. Print 1 if the device is read-only, 0 otherwise."
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:128
msgid "B<--getsize64>"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:130
msgid "Print device size in bytes."
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:133
msgid "B<--getsize>"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:135
msgid ""
"Print device size (32-bit!) in sectors. Deprecated in favor of the "
"B<--getsz> option."
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:138
msgid "B<--getss>"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:140
msgid "Print logical sector size in bytes - usually 512."
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:143
msgid "B<--getsz>"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:145
msgid "Get size in 512-byte sectors."
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:148
msgid "B<--rereadpt>"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:150
msgid "Reread partition table"
msgstr ""

#. type: Plain text
#: original/man8/blockdev.8:153
