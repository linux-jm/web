#: original/man1/cups-config.1:58
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: original/man1/cups-config.1:59
#, no-wrap
msgid "--api-version"
msgstr ""

#. type: Plain text
#: original/man1/cups-config.1:63
msgid "Displays the current API version (major.minor)."
msgstr ""

#. type: TP
#: original/man1/cups-config.1:63
#, no-wrap
msgid "--build"
msgstr ""

#. type: Plain text
#: original/man1/cups-config.1:67
