#: original/man1/libtool.1:5 original/man1/libtoolize.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "書式"

#. type: Plain text
#: original/man1/libtool.1:8
msgid "B<libtool> [I<\\,OPTION\\/>]... [I<\\,MODE-ARG\\/>]..."
msgstr "B<libtool> [I<\\,OPTION\\/>]... [I<\\,MODE-ARG\\/>]..."

#. type: SH
#: original/man1/libtool.1:8 original/man1/libtoolize.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "説明"

#. type: Plain text
#: original/man1/libtool.1:10
msgid "Provide generalized library-building support services."
msgstr "一般化されたライブラリビルドのためのサポートサービスを提供します。 "

#. type: SH
#: original/man1/libtool.1:10 original/man1/libtoolize.1:10
#, no-wrap
msgid "OPTIONS"
msgstr "オプション"

#. type: TP
#: original/man1/libtool.1:11
#, no-wrap
msgid "B<--config>"
msgstr "B<--config>"

#. type: Plain text
#: original/man1/libtool.1:14
msgid "show all configuration variables"
msgstr "設定変数をすべて表示します。"

#. type: TP
#: original/man1/libtool.1:14 original/man1/libtoolize.1:14
#: original/man1/libtoolize.1:76
#, no-wrap
msgid "B<--debug>"
msgstr "B<--debug>"

#. type: Plain text
#: original/man1/libtool.1:17 original/man1/libtoolize.1:17
#: original/man1/libtoolize.1:79
msgid "enable verbose shell tracing"
msgstr "詳細なシェルトレース出力を有効にします。"

#. type: TP
#: original/man1/libtool.1:17 original/man1/libtoolize.1:17
#, no-wrap
msgid "B<-n>, B<--dry-run>"
msgstr "B<-n>, B<--dry-run>"

#. type: Plain text
#: original/man1/libtool.1:20
msgid "display commands without modifying any files"
msgstr "ファイルへの修正は何も行わずに、コマンドだけ表示します。"

#. type: TP
#: original/man1/libtool.1:20
#, no-wrap
msgid "B<--features>"
msgstr "B<--features>"

#. type: Plain text
#: original/man1/libtool.1:23
msgid "display basic configuration information and exit"
msgstr "基本的な設定情報を表示して終了します。"

#. type: TP
#: original/man1/libtool.1:23
#, no-wrap
msgid "B<--mode>=I<\\,MODE\\/>"
msgstr "B<--mode>=I<\\,MODE\\/>"

#. type: Plain text
#: original/man1/libtool.1:26
msgid "use operation mode MODE"
msgstr "操作モードの中から MODE を利用します。"

#. type: TP
#: original/man1/libtool.1:26 original/man1/libtoolize.1:29
#: original/man1/libtoolize.1:79
#, no-wrap
msgid "B<--no-warnings>"
msgstr "B<--no-warnings>"

#. type: Plain text
#: original/man1/libtool.1:29 original/man1/libtoolize.1:32
msgid "equivalent to '-Wnone'"
msgstr "'-Wnone' と同様。"

#. type: TP
#: original/man1/libtool.1:29
#, no-wrap
msgid "B<--preserve-dup-deps>"
msgstr "B<--preserve-dup-deps>"

#. type: Plain text
#: original/man1/libtool.1:32
msgid "don't remove duplicate dependency libraries"
msgstr "重複した依存ライブラリを削除しません。"

#. type: TP
#: original/man1/libtool.1:32
#, no-wrap
msgid "B<--quiet>, B<--silent>"
msgstr "B<--quiet>, B<--silent>"

#. type: Plain text
#: original/man1/libtool.1:35
msgid "don't print informational messages"
msgstr "通常のメッセージを表示しません。"

#. type: TP
#: original/man1/libtool.1:35
#, no-wrap
msgid "B<--tag>=I<\\,TAG\\/>"
msgstr "B<--tag>=I<\\,TAG\\/>"

#. type: Plain text
#: original/man1/libtool.1:38
msgid "use configuration variables from tag TAG"
msgstr "設定変数を TAG で示されるタグから利用します。"

#. type: TP
#: original/man1/libtool.1:38 original/man1/libtoolize.1:44
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#: original/man1/libtool.1:41
msgid "print more informational messages than default"
msgstr "デフォルトよりも多くのメッセージを表示します。"

#. type: TP
#: original/man1/libtool.1:41 original/man1/libtoolize.1:47
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: original/man1/libtool.1:44
msgid "print version information"
msgstr "バージョン情報を表示します。"

#. type: TP
#: original/man1/libtool.1:44 original/man1/libtoolize.1:50
#, no-wrap
msgid "B<-W>, B<--warnings>=I<\\,CATEGORY\\/>"
msgstr "B<-W>, B<--warnings>=I<\\,CATEGORY\\/>"

#. type: Plain text
#: original/man1/libtool.1:47 original/man1/libtoolize.1:53
msgid "report the warnings falling in CATEGORY [all]"
msgstr "CATEGORY に該当する警告メッセージを表示します。 [デフォルト: all]"

#. type: TP
#: original/man1/libtool.1:47
#, no-wrap
msgid "B<-h>, B<--help>, B<--help-all>"
msgstr "B<-h>, B<--help>, B<--help-all>"

#. type: Plain text
#: original/man1/libtool.1:50
msgid "print short, long, or detailed help message"
msgstr "ヘルプメッセージをそれぞれ、短く、長く、詳細に表示します。"

#. type: SS
#: original/man1/libtool.1:50 original/man1/libtoolize.1:56
#, no-wrap
msgid "Warning categories include:"
msgstr "警告のカテゴリーは以下のとおりです。"

#. type: TP
#: original/man1/libtool.1:51 original/man1/libtoolize.1:57
#, no-wrap
msgid "\\&'all'"
msgstr "\\&'all'"

#. type: Plain text
#: original/man1/libtool.1:54 original/man1/libtoolize.1:60
msgid "show all warnings"
msgstr "警告メッセージをすべて表示します。"

#. type: TP
#: original/man1/libtool.1:54 original/man1/libtoolize.1:60
#, no-wrap
msgid "\\&'none'"
msgstr "\\&'none'"

#. type: Plain text
#: original/man1/libtool.1:57 original/man1/libtoolize.1:63
msgid "turn off all the warnings"
msgstr "警告メッセージをすべて非表示にします。"

#. type: TP
#: original/man1/libtool.1:57 original/man1/libtoolize.1:63
#, no-wrap
msgid "\\&'error'"
msgstr "\\&'error'"

#. type: Plain text
#: original/man1/libtool.1:60 original/man1/libtoolize.1:66
msgid "warnings are treated as fatal errors"
msgstr "警告を致命的エラーとして扱います。"

#. type: Plain text
#: original/man1/libtool.1:62
msgid "MODE must be one of the following:"
msgstr "MODE は以下のいずれかです。"

#. type: TP
#: original/man1/libtool.1:62
#, no-wrap
msgid "clean"
msgstr "clean"

#. type: Plain text
#: original/man1/libtool.1:65
msgid "remove files from the build directory"
msgstr "ビルドディレクトリ内のファイルを削除します。"

#. type: TP
#: original/man1/libtool.1:65
#, no-wrap
msgid "compile"
msgstr "compile"

#. type: Plain text
#: original/man1/libtool.1:68
msgid "compile a source file into a libtool object"
msgstr "ソースファイルをコンパイルして libtool オブジェクトを生成します。"

#. type: TP
#: original/man1/libtool.1:68
#, no-wrap
msgid "execute"
msgstr "execute"

#. type: Plain text
#: original/man1/libtool.1:71
msgid "automatically set library path, then run a program"
msgstr "ライブラリのパスを自動設定した上でプログラムを実行します。"

#. type: TP
#: original/man1/libtool.1:71
#, no-wrap
msgid "finish"
msgstr "finish"

#. type: Plain text
#: original/man1/libtool.1:74
msgid "complete the installation of libtool libraries"
msgstr "libtool ライブラリのインストールをすべて行います。"

#. type: TP
#: original/man1/libtool.1:74
#, no-wrap
msgid "install"
msgstr "install"

#. type: Plain text
#: original/man1/libtool.1:77
msgid "install libraries or executables"
msgstr "ライブラリと実行ファイルをインストールします。"

#. type: TP
#: original/man1/libtool.1:77
#, no-wrap
msgid "link"
msgstr "link"

#. type: Plain text
#: original/man1/libtool.1:80
msgid "create a library or an executable"
msgstr "ライブラリまたは実行ファイルを生成します。"

#. type: TP
#: original/man1/libtool.1:80
#, no-wrap
msgid "uninstall"
msgstr "uninstall"

#. type: Plain text
#: original/man1/libtool.1:83
msgid "remove libraries from an installed directory"
msgstr "インストールディレクトリからライブラリを削除します。"

#. type: Plain text
#: original/man1/libtool.1:86
msgid ""
"MODE-ARGS vary depending on the MODE.  When passed as first option, \\&'--"
"mode=MODE' may be abbreviated as 'MODE' or a unique abbreviation of that."
msgstr ""
"MODE-ARGS は MODE に応じて変わります。 1 つめのオプションとして受け渡される場"
"合は、 \\&'--mode=MODE' は 'MODE' として省略できます。 あるいはその短縮形とす"
"ることもできます。 "

#. type: Plain text
#: original/man1/libtool.1:88 original/man1/libtoolize.1:112
msgid "GNU libtool home page: E<lt>http://www.gnu.org/software/libtool/E<gt>."
msgstr ""
"GNU libtool ホームページ: E<lt>http://www.gnu.org/software/libtool/E<gt>."

#. type: Plain text
#: original/man1/libtool.1:91 original/man1/libtoolize.1:94
msgid ""
"When reporting a bug, please describe a test case to reproduce it and "
"include the following information:"
msgstr ""
"バグを報告する際には、 バグが再現できるテストケースを示した上で、 以下の情報"
"を含めてください。"

#. type: TP
#: original/man1/libtool.1:91 original/man1/libtoolize.1:94
#, no-wrap
msgid "host-triplet:"
msgstr "ホストのトリプレット (triplet):"

#. type: Plain text
#: original/man1/libtool.1:94 original/man1/libtoolize.1:97
msgid "x86_64-pc-linux-gnu"
msgstr "x86_64-pc-linux-gnu"

#. type: TP
#: original/man1/libtool.1:94
#, no-wrap
msgid "shell:"
msgstr "シェル:"

#. type: Plain text
#: original/man1/libtool.1:97
msgid "I<\\,/bin/bash\\/>"
msgstr "I<\\,/bin/bash\\/>"

#. type: TP
#: original/man1/libtool.1:97
#, no-wrap
msgid "compiler:"
msgstr "コンパイラー:"

#. type: Plain text
#: original/man1/libtool.1:100
msgid "gcc"
msgstr "gcc"

#. type: Plain text
#: original/man1/libtool.1:106
msgid ""
"compiler flags: B<-g> B<-O2> linker: I<\\,/usr/bin/ld\\/> B<-m> elf_x86_64 "
"(gnu? yes)  version: libtool (GNU libtool) 2.4.7 automake: automake (GNU "
"automake) 1.16.3 autoconf: autoconf (GNU Autoconf) 2.69"
msgstr ""
"コンパイラーフラグ: B<-g> B<-O2> リンカー: I<\\,/usr/bin/ld\\/> B<-m> elf_x86_64 "
"(gnu かどうか? yes)  version: libtool (GNU libtool) 2.4.7 automake: automake (GNU "
"automake) 1.16.3 autoconf: autoconf (GNU Autoconf) 2.69"

#. type: SH
#: original/man1/libtool.1:106 original/man1/libtoolize.1:106
#, no-wrap
msgid "AUTHOR"
msgstr "著者"

#. type: Plain text
#: original/man1/libtool.1:108
msgid "Written by Gordon Matzigkeit, 1996"
msgstr "Gordon Matzigkeit, 1996"

#. type: SH
#: original/man1/libtool.1:108 original/man1/libtoolize.1:108
#, no-wrap
msgid "REPORTING BUGS"
msgstr "バグ報告"

#. type: Plain text
#: original/man1/libtool.1:110 original/man1/libtoolize.1:110
msgid "Report bugs to E<lt>bug-libtool@gnu.orgE<gt>."
msgstr "バグ報告は E<lt>bug-libtool@gnu.orgE<gt> までお願いします。"

#. type: SH
#: original/man1/libtool.1:110 original/man1/libtoolize.1:114
#, no-wrap
msgid "COPYRIGHT"
msgstr "著作権"

#. type: Plain text
#: original/man1/libtool.1:112
msgid "Copyright \\(co 2014 Free Software Foundation, Inc."
msgstr "Copyright \\(co 2014 Free Software Foundation, Inc."

#. type: SH
#: original/man1/libtool.1:112 original/man1/libtoolize.1:116
#, no-wrap
msgid "SEE ALSO"
msgstr "関連項目"

#. type: Plain text
#: original/man1/libtool.1:120
msgid ""
"The full documentation for B<libtool> is maintained as a Texinfo manual.  If "
"the B<info> and B<libtool> programs are properly installed at your site, the "
"command"
msgstr ""
"B<libtool> の完全なドキュメントは Texinfo マニュアルとしてメンテナンスされて"
"います。B<info> と B<libtool> の両プログラムが適切にインストールされていれ"
"ば、以下のコマンド"

#. type: Plain text
#: original/man1/libtool.1:122
msgid "B<info libtool>"
msgstr "B<info libtool>"

#. type: Plain text
#: original/man1/libtool.1:123 original/man1/libtoolize.1:127
msgid "should give you access to the complete manual."
msgstr "を実行して完全なマニュアルを参照できます。"

#. type: TH
#: original/man1/libtoolize.1:2
#, no-wrap
msgid "LIBTOOLIZE"
msgstr "LIBTOOLIZE"

#. type: Plain text
#: original/man1/libtoolize.1:5
msgid "libtoolize - manual page for libtoolize 2.4.7"
msgstr "libtoolize - libtoolize 2.4.7 向けのマニュアルページ"

#. type: Plain text
#: original/man1/libtoolize.1:8
msgid "B<libtoolize> [I<\\,OPTION\\/>]..."
msgstr "B<libtoolize> [I<\\,OPTION\\/>]..."

#. type: Plain text
#: original/man1/libtoolize.1:10
msgid "Prepare a package to use libtool."
msgstr "libtool において利用できるようにパッケージを準備します。"

#. type: TP
#: original/man1/libtoolize.1:11
#, no-wrap
msgid "B<-c>, B<--copy>"
msgstr "B<-c>, B<--copy>"

#. type: Plain text
#: original/man1/libtoolize.1:14
msgid "copy files rather than symlinking them"
msgstr "ファイルのシンボリックリンクを生成せず、 ファイルをコピーします。"

#. type: Plain text
#: original/man1/libtoolize.1:20
msgid "print commands rather than running them"
msgstr "コマンドを実行せず、 コマンドの表示だけを行います。"

#. type: TP
#: original/man1/libtoolize.1:20
#, no-wrap
msgid "B<-f>, B<--force>"
msgstr "B<-f>, B<--force>"

#. type: Plain text
#: original/man1/libtoolize.1:23
msgid "replace existing files"
msgstr "既存のファイルを置き換えます。"

#. type: TP
#: original/man1/libtoolize.1:23
#, no-wrap
msgid "B<-i>, B<--install>"
msgstr "B<-i>, B<--install>"

#. type: Plain text
#: original/man1/libtoolize.1:26
msgid "copy missing auxiliary files"
msgstr "補助的なファイルがなければコピーします。"

#. type: TP
#: original/man1/libtoolize.1:26
#, no-wrap
msgid "B<--ltdl>[=I<\\,DIR\\/>]"
msgstr "B<--ltdl>[=I<\\,DIR\\/>]"

#. type: Plain text
#: original/man1/libtoolize.1:29
msgid "install libltdl sources [default: libltdl]"
msgstr "libltdl ソースをインストールします。 [デフォルト: libltdl]"

#. type: TP
#: original/man1/libtoolize.1:32
#, no-wrap
msgid "B<--nonrecursive>"
msgstr "B<--nonrecursive>"

#. type: Plain text
#: original/man1/libtoolize.1:35
msgid "prepare ltdl for non-recursive make"
msgstr "非再帰的 (non-recursive) な make 向けに ltdl を用意します。 "

#. type: TP
#: original/man1/libtoolize.1:35
#, no-wrap
msgid "B<-q>, B<--quiet>"
msgstr "B<-q>, B<--quiet>"

#. type: Plain text
#: original/man1/libtoolize.1:38 original/man1/libtoolize.1:85
msgid "work silently"
msgstr "メッセージ出力を行いません。"

#. type: TP
#: original/man1/libtoolize.1:38
#, no-wrap
msgid "B<--recursive>"
msgstr "B<--recursive>"

#. type: Plain text
#: original/man1/libtoolize.1:41
msgid "prepare ltdl for recursive make"
msgstr "再帰的 (recursive) な make 向けに ltdl を用意します。"

#. type: TP
#: original/man1/libtoolize.1:41
#, no-wrap
msgid "B<--subproject>"
msgstr "B<--subproject>"

#. type: Plain text
#: original/man1/libtoolize.1:44
msgid "prepare ltdl to configure and build independently"
msgstr "ltdl の設定とビルドを独立して用意します。"

#. type: Plain text
#: original/man1/libtoolize.1:47 original/man1/libtoolize.1:88
msgid "verbosely report processing"
msgstr "詳細なメッセージ出力を行います。"

#. type: Plain text
#: original/man1/libtoolize.1:50
msgid "print version information and exit"
msgstr "バージョン情報を表示して終了します。"

#. type: TP
#: original/man1/libtoolize.1:53
#, no-wrap
msgid "B<-h>, B<--help>"
msgstr "B<-h>, B<--help>"

#. type: Plain text
#: original/man1/libtoolize.1:56
msgid "print short or long help message"
msgstr "短いヘルプメッセージ、または長いヘルプメッセージを表示します。"

#. type: TP
#: original/man1/libtoolize.1:66
#, no-wrap
msgid "\\&'environment'"
msgstr "\\&'environment'"

#. type: Plain text
#: original/man1/libtoolize.1:69
msgid "show warnings about LIBTOOLIZE_OPTIONS content"
msgstr "LIBTOOLIZE_OPTIONS の内容についての警告を表示します。"

#. type: TP
#: original/man1/libtoolize.1:69
#, no-wrap
msgid "\\&'file'"
msgstr "\\&'file'"

#. type: Plain text
#: original/man1/libtoolize.1:72
msgid "show warnings about file copying and linking"
msgstr "ファイルコピーおよびリンクについての警告を表示します。"

#. type: Plain text
#: original/man1/libtoolize.1:76
msgid ""
"The following space or comma delimited options can be passed to libtoolize "
"via the environment variable LIBTOOLIZE_OPTIONS, unknown environment options "
"are ignored:"
msgstr ""
"オプション間を区切る空白文字やカンマは、 環境変数 LIBTOOLIZE_OPTIONS を通じ"
"て、 libtoolize に受け渡されます。 不明な環境オプションは無視されます。 "

#. type: Plain text
#: original/man1/libtoolize.1:82
msgid "don't display warning messages"
msgstr "警告メッセージを表示しません。"

#. type: TP
#: original/man1/libtoolize.1:82
#, no-wrap
msgid "B<--quiet>"
msgstr "B<--quiet>"

#. type: TP
#: original/man1/libtoolize.1:85
#, no-wrap
msgid "B<--verbose>"
msgstr "B<--verbose>"

#. type: Plain text
#: original/man1/libtoolize.1:91
msgid ""
"You must 'cd' to the top directory of your package before you run "
"\\&'libtoolize'."
msgstr ""
"\\&'libtoolize' を実行するにあたっては、パッケージのトップディレクトリに "
"'cd' により移動しておくことが必要です。"

#. type: TP
#: original/man1/libtoolize.1:97
#, no-wrap
msgid "version:"
msgstr "バージョン:"

