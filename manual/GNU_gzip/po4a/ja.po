#: original/man1/gzip.1:4
msgid "gzip, gunzip, zcat - compress or expand files"
msgstr "gzip, gunzip, zcat - ファイルの圧縮、伸長を行う"

#. type: Plain text
#: original/man1/gzip.1:12
msgid "B<gzip> [B< -acdfhklLnNrtvV19 >] [B<-S\\ suffix>] [ I<name \\&...> ]"
msgstr "B<gzip> [B< -acdfhklLnNrtvV19 >] [B<-S\\ suffix>] [ I<name \\&...> ]"

#. type: Plain text
#: original/man1/gzip.1:20
msgid "B<gunzip> [B< -acfhklLnNrtvV >] [B<-S\\ suffix>] [ I<name \\&...> ]"
msgstr "B<gunzip> [B< -acfhklLnNrtvV >] [B<-S\\ suffix>] [ I<name \\&...> ]"

