\fBpasswd.byname\fP
.TP 
\fBgroup\fP
\fBgroup.byname\fP
.TP 
\fBnetworks\fP
\fBnetworks.byaddr\fP
.TP 
\fBhosts\fP
\fBhosts.byname\fP
.TP 
\fBprotocols\fP
\fBprotocols.bynumber\fP
.TP 
\fBservices\fP
\fBservices.byname\fP
.TP 
\fBaliases\fP
\fBmail.aliases\fP
.TP 
\fBethers\fP
\fBethers.byname\fP
.LP
